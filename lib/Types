export type Song = {
  songUrl: string;
  artist: string;
  title: string;
};

export type NowPlayingSong = {
  album: string;
  albumImageUrl: string;
  artist: string;
  isPlaying: boolean;
  songUrl: string;
  title: string;
};

export type RecentlyPlayed = {
    album: string;
    albumImageURL: string;
    artist: string;
    songUrl: string;
    title: string;
}